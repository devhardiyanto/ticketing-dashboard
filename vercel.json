{
	"version": 2,
	"framework": "laravel",
	"builds": [
		{ "src": "/public/index.php", "use": "vercel-php@0.6.0" },
		{ "src": "/package.json", "use": "@vercel/static-build" }
	],
	"routes": [
		{ "src": "/build/(.*)", "dest": "/public/build/$1" },
		{ "src": "/(css|js|images|storage)/(.*)", "dest": "/public/$1/$2" },
		{ "src": "/(.*)", "dest": "/public/index.php" }
	]
}
